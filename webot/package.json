    "apollo-link-ws": "^1.0.20",
    "async": "^3.1.0",
    "axios": "^0.19.0",
    "axios-debug-log": "^0.6.2",
    "bcrypt": "^4.0.1",
    "bignumber.js": "^9.0.0",
    "body-parser": "^1.19.0",
    "caporal": "^1.3.0",
    "commander": "^4.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "csv-parse": "^4.8.2",
    "date-fns": "^2.10.0",
    "delay": "^4.3.0",
    "express": "^4.17.1",
    "fast-csv": "^3.4.0",
